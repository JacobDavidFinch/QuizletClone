{
  "name": "quizletclone",
  "version": "1.0.0",
  "description": "Clone of one of my favorite applications - Quizlet",
  "main": "index.js",
  "engines": {
    "node": "12.18.2"
  },
  "scripts": {
    "build": "babel --delete-dir-on-start --out-dir dist --copy-files --ignore \"**/__tests__/**,**/__mocks__/**\" --no-copy-ignored src",
    "dev": "npm-run-all --parallel api:dev client",
    "start": "npm-run-all --parallel api client",
    "api": "node index.js",
    "client": "cd .. && cd client && npm run start",
    "clientbuild": "cd .. && client && npm run build",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JacobDavidFinch/QuizletClone.git"
  },
  "author": "Jacob David Dykstra",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "loglevel": "^1.6.8",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.0.0",
    "npm": "^6.11.3",
    "path": "^0.12.7",
    "bcryptjs": "^2.4.3",
    "express-jwt": "^5.3.3",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.4",
    "nodemon": "^2.0.4"
  },
  "bugs": {
    "url": "https://github.com/JacobDavidFinch/QuizletClone/issues"
  },
  "homepage": "https://github.com/JacobDavidFinch/QuizletClone#readme"
}
