{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "client": "cd ../ && cd client && npm run start",
    "client-build": "cd client && npm run build",
    "dev": "concurrently \"npm run client\" \"npm run start\"",
    "start": "npm run watchnode",
    "test": "jest --watch",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/index.js",
    "watchnode": "tsnd --respawn src/index.ts"
  },
  "dependencies": {
    "atob": "^2.1.2",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.2",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "log4js": "^6.3.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "concurrently": "^4.1.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/node": "^14.0.27",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "typescript": "^3.9.7"
  }
}
